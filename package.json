{
  "name": "lagden-tooltip",
  "version": "1.0.1",
  "description": "Tooltip do Osvaldas",
  "keywords": [
    "tooltip",
    "bubble"
  ],
  "bug": {
    "url": "https://github.com/lagden/tooltip/issues",
    "email": "lagden@gmail.com"
  },
  "license": "MIT",
  "author": {
    "name": "Thiago Lagden",
    "email": "lagden@gmail.com",
    "url": "http://lagden.in"
  },
  "files": [
    "dist",
    "es6"
  ],
  "main": "dist/tooltip.js",
  "repository": "lagden/tooltip",
  "scripts": {
    "pretest": "xo",
    "test": "mocha --reporter spec --require babel-hook --require should",
    "start": "grunt serve",
    "build": "grunt build",
    "prepublish": "grunt build && npm test"
  },
  "dependencies": {
    "lagden-utils": "^0.4.0"
  },
  "devDependencies": {
    "almond": "^0.3.1",
    "autoprefixer": "^6.0.3",
    "babel": "^5.8.23",
    "grunt": "^0.4.5",
    "grunt-babel": "^5.0.0",
    "grunt-browser-sync": "^2.1.3",
    "grunt-concurrent": "^2.0.3",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-coffee": "^0.13.0",
    "grunt-contrib-copy": "^0.8.0",
    "grunt-contrib-cssmin": "^0.14.0",
    "grunt-contrib-jade": "^0.15.0",
    "grunt-contrib-requirejs": "^0.4.4",
    "grunt-contrib-stylus": "^0.22.0",
    "grunt-contrib-symlink": "^0.3.0",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-postcss": "^0.6.0",
    "grunt-xo": "^0.6.0",
    "jade": "^1.11.0",
    "jit-grunt": "^0.9.1",
    "jquery": "^2.1.4",
    "jsdom": "^6.5.1",
    "mocha": "^2.3.3",
    "requirejs": "^2.1.18",
    "should": "^7.1.0",
    "time-grunt": "^1.2.0",
    "xo": "^0.9.0"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "xo": {
    "envs": [
      "node"
    ],
    "esnext": true,
    "ignores": [
      "dist/**",
      "build/**",
      "dev/**"
    ],
    "global": [
      "window"
    ],
    "rules": {
      "prefer-reflect": [
        2,
        {
          "exceptions": [
            "apply",
            "call",
            "defineProperty",
            "getOwnPropertyDescriptor",
            "getPrototypeOf",
            "setPrototypeOf",
            "isExtensible",
            "getOwnPropertyNames",
            "preventExtensions",
            "delete"
          ]
        }
      ],
      "new-cap": [
        2,
        {
          "newIsCap": true,
          "capIsNew": false
        }
      ]
    }
  },
  "babel": {
    "modules": "umd",
    "stage": 1
  }
}
